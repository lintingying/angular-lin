{
  "name": "my-lib",
  "version": "0.0.1",
  "peerDependencies": {
    "@angular/common": "^10.2.4",
    "@angular/core": "^10.2.4"
  },
  "dependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  },
  "scripts": {
    "build": "node ../../node_modules/typescript tsc -p tsconfig.schematics.json",
    "postbuild": "node ../../node_modules/copyfiles copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/my-lib/"
  },
  "ng-add": {
    "save": "devDependencies"
  }
}
