{
  "name": "ng-lin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "themes:build": "node --max_old_space_size=4096 src/scripts/themes.js",
    "themes-compact:build": "node --max_old_space_size=4096 src/scripts/themes-compact.js",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build ng-lin --prod --sourceMap=false",
    "build:with:stats": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build ng-lin --prod --source-map --stats-json",
    "analyzer": "webpack-bundle-analyzer wwwroot/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@microsoft/signalr": "^6.0.2",
    "angular-web-storage": "^4.1.0",
    "ng-zorro-antd": "10.2.2",
    "qiankun": "^2.6.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^6.0.0",
    "core-js": "^3.20.3",
    "custom-event-polyfill": "^1.0.7",
    "husky": "^7.0.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "less": "^3.11.1",
    "less-plugin-clean-css": "^1.5.1",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.2",
    "whatwg-fetch": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng lint ng-lin"
    }
  }
}
